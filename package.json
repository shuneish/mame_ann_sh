{
  "name": "a-frame-react-demo",
  "version": "1.0.0",
  "description": "A-Frame React TypeScript Demo with WASM acceleration",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:wasm && react-scripts build",
    "build:wasm": "cd src/wasm && wasm-pack build --target web",
    "dev:wasm": "cd src/wasm && wasm-pack build --target web --dev",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "aframe": "^1.4.2",
    "aframe-ar": "^0.2.1-a",
    "aframe-react": "^4.4.0",
    "firebase": "^12.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/aframe": "^1.2.8",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.0",
    "wasm-pack": "^0.12.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
